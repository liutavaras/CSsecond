<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main14"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.liutaurasmazonas.cslogintrying.HomePageNews">

<!--:app/src/main/res/layout/home_page_newsxml-->

    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/recyclerview"
    />


    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/news"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:id="@+id/imageButton2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/liverates"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:id="@+id/imageButton5"


        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2"
        android:layout_marginLeft="71dp"
        android:layout_marginStart="71dp" />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/settingsbuttongrey"
        android:scaleType="fitCenter"
        android:padding="0dp"
        android:id="@+id/imageButton6"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton5"
        android:layout_toEndOf="@+id/imageButton5" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/economiccalendarbuttongrey"
        android:id="@+id/imageButton3"
        android:scaleType="fitCenter"
        android:padding="0dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/imageButton6"
        android:layout_toStartOf="@+id/imageButton6"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="83dp" />


    <TextView
        android:text="hello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="219dp"
        android:textColor="#4286f4"
        android:id="@+id/textView" />

</RelativeLayout>
