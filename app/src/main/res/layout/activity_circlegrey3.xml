<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.liutaurasmazonas.cslogintrying.Circlegrey3">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="wrap_content"
        android:layout_height="77dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">

        <ImageButton
            android:id="@+id/ibEconCalBlack"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="90dp"
            android:background="#00ffffff"
            android:cropToPadding="false"
            android:scaleType="fitEnd"
            app:srcCompat="@drawable/economiccalendarbuttongrey2" />

        <ImageButton
            android:id="@+id/ibLiveRates"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/ibEconCalBlack"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            app:srcCompat="@drawable/liveratesbuttongrey2" />

        <ImageButton
            android:id="@+id/ibNewsBlack"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            app:srcCompat="@drawable/newsbutton2" />

        <ImageButton
            android:id="@+id/ibSettingsBlack"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            app:srcCompat="@drawable/settingsbuttongrey2" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="470dp"
        android:layout_above="@+id/relativeLayout"
        android:layout_alignParentStart="true">

        <ImageButton
            android:id="@+id/buttonsetpriority"
            android:layout_width="wrap_content"
            android:layout_height="153dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="#00ffffff"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/circlesbuttonsetpriority4" />

        <ImageButton
            android:id="@+id/buttonlookforclient"
            android:layout_width="wrap_content"
            android:layout_height="153dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/buttonsetpriority"
            android:layout_marginTop="3dp"
            android:background="#00ffffff"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/clientlist5" />

        <ImageButton
            android:id="@+id/buttonclearcache"
            android:layout_width="wrap_content"
            android:layout_height="158dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/buttonlookforclient"
            android:layout_marginTop="3dp"
            android:background="#00ffffff"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/remove5" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout40"
        android:layout_width="match_parent"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/imageView50"
            android:layout_width="790dp"
            android:layout_height="65dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:scaleType="matrix"
            app:srcCompat="@drawable/topbarcremebackgroundddd2" />

        <ImageButton
            android:id="@+id/addAProfile"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignBottom="@+id/imageView50"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="13dp"
            android:layout_marginLeft="14dp"
            android:background="#00ffffff"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/pluss" />


    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">


        <EditText
            android:id="@+id/editTextInput"
            android:layout_width="230dp"
            android:layout_height="35dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="55dp"
            android:layout_marginTop="16dp"
            android:background="@color/cardview_light_background">


        </EditText>

    </RelativeLayout>
    <ImageButton
        android:id="@+id/button1"
        android:layout_width="34dp"
        android:layout_height="35dp"
        android:layout_marginEnd="21dp"
        android:layout_marginTop="16dp"
        android:background="#00ffffff"
        android:onClick="onSearchClick"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/search"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>