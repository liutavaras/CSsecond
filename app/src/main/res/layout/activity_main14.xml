<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity14">
    <Button
        android:id="@+id/buttonUrl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/red_button"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:text="Economic Calendar" />
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.example.liutaurasmazonas.cslogintrying.MainActivity14">


    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/recyclerview"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="95dp">

    </android.support.v7.widget.RecyclerView>


    <!--<ImageView-->
        <!--android:id="@+id/imageView3"-->
        <!--android:layout_width="3000dp"-->
        <!--android:layout_height="65dp"-->
        <!--android:adjustViewBounds="true"-->
        <!--android:padding="0dp"-->
        <!--app:srcCompat="@drawable/bgfornav"-->
        <!--android:layout_alignParentBottom="true"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_alignParentStart="true"-->
        <!--android:scaleType="fitXY"-->
        <!--/>-->

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/newbuttonblue2"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:id="@+id/imageButton2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#0000"
        />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/liveratesbuttongrey2"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:id="@+id/imageButton5"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2"
        android:layout_marginLeft="71dp"
        android:layout_marginStart="71dp"
        android:background="#0000"
        />

    <ImageButton
        android:layout_width="95dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/settingsbuttongrey2"
        android:scaleType="fitCenter"
        android:padding="0dp"
        android:id="@+id/imageButton6"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton5"
        android:layout_toEndOf="@+id/imageButton5"
        android:background="#0000"
        />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="90dp"
        android:layout_height="65dp"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="83dp"
        android:layout_marginRight="10dp"
        android:layout_toLeftOf="@+id/imageButton6"
        android:layout_toStartOf="@+id/imageButton6"
        android:background="#0000"
        android:padding="0dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/economiccalendarbuttongrey2" />

    <ImageButton
        android:id="@+id/googleImgBtn"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitCenter"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#0000"
        android:padding="0dp"
        android:layout_marginLeft="9dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="9dp"
        app:srcCompat="@drawable/circlesgreen" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitCenter"
        android:background="#0000"
        android:padding="0dp"
        app:srcCompat="@drawable/circlesamber"
        android:layout_alignTop="@+id/googleImgBtn"
        android:layout_alignRight="@+id/imageButton3"
        android:layout_alignEnd="@+id/imageButton3" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitCenter"
        android:background="#0000"
        android:padding="0dp"
        app:srcCompat="@drawable/circlesredvodafone"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_toRightOf="@+id/imageButton4"
        android:layout_toEndOf="@+id/imageButton4" />


</RelativeLayout>


