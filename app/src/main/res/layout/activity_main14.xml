<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main14"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.example.liutaurasmazonas.cslogintrying.MainActivity14">


    <android.support.v7.widget.RecyclerView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/recyclerview"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="95dp">

    </android.support.v7.widget.RecyclerView>


    <!--<ImageView-->
    <!--android:id="@+id/imageView3"-->
    <!--android:layout_width="3000dp"-->
    <!--android:layout_height="65dp"-->
    <!--android:adjustViewBounds="true"-->
    <!--android:padding="0dp"-->
    <!--app:srcCompat="@drawable/bgfornav"-->
    <!--android:layout_alignParentBottom="true"-->
    <!--android:layout_alignParentLeft="true"-->
    <!--android:layout_alignParentStart="true"-->
    <!--android:scaleType="fitXY"-->
    <!--/>-->

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/news"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:id="@+id/imageButton2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#0000"
        />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/liverates"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        android:id="@+id/imageButton5"
        android:layout_marginLeft="71dp"
        android:layout_marginStart="71dp"
        android:background="#0000"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2" />

    <RelativeLayout
        android:layout_width="300dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <ImageButton
            app:srcCompat="@drawable/newsbutton"
            android:scaleType="fitEnd"
            android:background="#00ffffff"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/ibNewsBlack" />

        <ImageButton
            app:srcCompat="@drawable/economiccalendarbuttongrey"
            android:id="@+id/ibEconCalBlack"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:cropToPadding="false"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="90dp"
            android:layout_marginStart="90dp" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/liveratesbuttonblue"
            android:id="@+id/ibLiveRates"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/ibEconCalBlack"
            android:layout_toEndOf="@+id/ibEconCalBlack" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/settingsbuttongrey"
            android:id="@+id/ibSettingsBlack"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>
    <ImageButton
        android:id="@+id/googleImgBtn"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitCenter"
        android:background="#0000"
        android:padding="0dp"
        android:layout_marginTop="9dp"
        app:srcCompat="@drawable/circlesgreen"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitCenter"
        android:background="#0000"
        android:padding="0dp"
        app:srcCompat="@drawable/circlesamber"
        android:layout_alignTop="@+id/googleImgBtn"
        android:layout_alignLeft="@+id/imageButton5"
        android:layout_alignStart="@+id/imageButton5" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitCenter"
        android:background="#0000"
        android:padding="0dp"
        app:srcCompat="@drawable/circlesredvodafone"
        android:layout_alignTop="@+id/googleImgBtn"
        android:layout_toLeftOf="@+id/imageButton4"
        android:layout_toStartOf="@+id/imageButton4" />


</RelativeLayout>